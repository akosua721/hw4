<h1>Places</h1>

<p>
  <a href="/places/new" class="btn btn-outline-dark" >New place</a>
</p>

<ul class="list-unstyled">
  <% for place in @places %>
    <li>
      <a href="/places/<%= place["id"] %>" class="text-decoration-underline fw-bold text-primary h5 d-block"><%= place["name"] %></a>
    </li>
  <% end %>
</ul>